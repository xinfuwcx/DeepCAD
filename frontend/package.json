{
  "name": "deepcad-frontend",
  "private": true,
  "version": "3.0.0",
  "description": "DeepCAD震撼3D地球控制中心 - NASA级渲染 + 双AI助手系统",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5310 --strictPort",
    "dev:5202": "vite --port 5202 --strictPort",
    "dev:5310": "vite --port 5310 --strictPort",
    "build": "vite build",
    "build:typed": "tsc --project tsconfig.build.json --noEmit && vite build",
    "build:full": "tsc && vite build",
    "build:analyze": "vite build && npx source-map-explorer dist/assets/*.js",
    "size": "size-limit",
    "type:core": "tsc --project tsconfig.build.json --noEmit",
    "type:full": "tsc --noEmit",
    "type:perf": "tsc --noEmit src/utils/performanceMonitor.ts",
    "dev:pv3d": "vite --port 5311 --strictPort --open pv3d.html",
    "lint:core": "node scripts/run-lint-core.mjs",
    "analyze:unused": "node scripts/analyze-unused.mjs",
    "analyze:unused:json": "node scripts/analyze-unused.mjs --json > unused-report.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "1.0.1",
    "@ant-design/icons": "^5.6.1",
    "@deck.gl/aggregation-layers": "9.1.14",
    "@deck.gl/core": "9.1.14",
    "@deck.gl/extensions": "9.1.14",
    "@deck.gl/geo-layers": "9.1.14",
    "@deck.gl/layers": "9.1.14",
    "@deck.gl/mapbox": "9.1.14",
    "@deck.gl/mesh-layers": "9.1.14",
    "@deck.gl/react": "9.1.14",
    "@hookform/resolvers": "^3.10.0",
    "@react-three/drei": "9.105.0",
    "@react-three/fiber": "8.15.16",
    "@react-three/postprocessing": "2.16.0",
    "@types/maplibre-gl": "1.13.2",
    "antd": "^5.26.7",
    "axios": "^1.7.9",
    "clsx": "2.1.1",
    "d3": "^7.9.0",
    "dxf-parser": "1.1.2",
    "framer-motion": "^11.18.2",
    "immer": "^10.1.1",
    "lucide-react": "0.534.0",
    "maplibre-gl": "5.6.2",
    "papaparse": "5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "14.3.8",
    "react-hook-form": "^7.54.2",
    "react-i18next": "15.6.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "3.3.1",
    "three": "0.165.0",
    "three-csg-ts": "3.2.0",
    "uuid": "11.1.0",
  "zod": "^3.25.76",
  "xlsx": "0.18.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@size-limit/file": "11.2.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/d3": "7.4.3",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "jsdom": "^24.1.3",
    "size-limit": "11.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.19",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.9"
  }
}
